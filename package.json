{
  "name": "launchpad-contracts",
  "version": "1.0.0",
  "description": "This project contains the smart contracts and libraries required to launch multiple token presales.",
  "scripts": {
    "lint": "prettier --write **.sol",
    "solhint": "solhint 'contracts/**/*.sol'",
    "compile": "npx hardhat compile",
    "test": "npx hardhat --network hardhat test",
    "deploy-testnet": "npx hardhat run --network goerli scripts/deploy.ts",
    "verify-testnet": "npx hardhat run --network goerli scripts/verify.ts",
    "deploy-mainnet": "npx hardhat run --network mainnet scripts/deploy.ts",
    "verify-mainnet": "npx hardhat run --network mainnet scripts/verify.ts"
  },
  "keywords": [],
  "author": "guvenemirhan",
  "license": "ISC",
  "devDependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.0",
    "hardhat-gas-reporter": "^1.0.9",
    "install": "^0.13.0",
    "npm": "^9.6.2",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.3",
    "dotenv": "^16.0.3"
  },
  "directories": {
    "test": "test"
